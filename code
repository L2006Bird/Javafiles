    <script>
        const iframe = document.getElementById("sketchfab-iframe");
        const button = document.getElementById("changeIframe");
        
        const iframeUrls = [
            "https://sketchfab.com/models/62ed88b0dc40427a969d1bc1c8f822dc/embed",
            "https://sketchfab.com/models/7da13d1a773d43ecad737faf2c32d813/embed"
        ];
        
        let currentIndex = 0;

        button.addEventListener("click", () => {
            // Update the src attribute of the iframe with the next URL in the array
            iframe.src = iframeUrls[currentIndex];
            
            // Toggle the currentIndex between 0 and 1
            currentIndex = (currentIndex === 0) ? 1 : 0;
        });
    </script>
